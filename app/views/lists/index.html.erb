<div class="container">
  <h1>All lists</h1>
  <p><%= link_to 'Add a new list', new_list_path, class: 'btn btn-primary' %></p>
  <ul>
    <% @lists.each do |list| %>
    <li>
      <p><%= list.name %></p>
      <%= cl_image_tag list.photo.key, width: 400, height: 300, crop: :fill %>
    </li>
    <% end %>
  </ul>
</div>
